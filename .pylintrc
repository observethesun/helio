[MASTER]
extension-pkg-whitelist=numpy
init-hook='import sys; sys.path.append(".")'

[FORMAT]
max-line-length=120
max-attributes=8
max-args=10
max-locals=25
good-names=a,ax,b,B0,c,d,dx,dy,dz,d0,d1,d2,df,dt,i,ik,f,fn,j,jk,k,L0,lr,n,nx,ny,nz,p,q,r,s,s0,s1,s2,t,v,v1,v2,v3,x,xc,xy,x0,y,yc,y0,z,z0

[MESSAGE CONTROL]
disable=no-value-for-parameter,no-self-use,too-few-public-methods,unsubscriptable-object,no-method-argument,no-member,arguments-differ,len-as-condition,keyword-arg-before-vararg,too-many-locals,comparison-with-callable,not-an-iterable,unnecessary-lambda

[TYPECHECK]
ignored-modules=numpy

[MISCELLANEOUS]
notes=